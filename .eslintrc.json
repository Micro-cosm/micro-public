{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "arrow-parens": "off",
        "block-spacing": "off",
        "brace-style": "off",
        "comma-dangle": "off",
        "no-tabs": "off",
        "indent": "off",
        "key-spacing": "off",
        "no-multiple-empty-lines": "off",
        "new-cap": "off",
        "max-len": [ "warn",
            {
                "code": 150,
                "ignoreComments": true,
                "ignoreTrailingComments": true,
                "ignoreUrls": true
            }
        ],
        "object-curly-spacing": "off",
        "require-jsdoc": "off"
    }
}
